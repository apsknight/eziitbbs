{
    "name": "eziitbbs",
    "version": "1.0.0",
    "description": "Official IIT Bhubaneswar website made easy.",
    "manifest_version": 2,

    "browser_action": {
        "default_icon": {

        },
        "default_title": "eziitbbs",
        "default_popup": "popup.html"
    },

    "content_scripts": [
        {
            "matches": ["http://www.iitbbs.ac.in/*", "https://www.iitbbs.ac.in/*"],
            "js": ["extendNav.js"]
        }
    ],

    "permissions": [
        "https://iitbbs.herokuapp.com/"
    ]
}